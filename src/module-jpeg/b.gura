import(jpeg)

PrintIFD(ifd:jpeg.IFD, indentLevel:Number = 0) = {
	ifd.EachTag {|tag|
		Printf('%*s%s(%04x) [%s]: %s\n',
			indentLevel * 2, '', tag.name, tag.tagId,
			tag.type.name, tag.value)
		if (tag.type == `IFD) {
			PrintIFD(tag.value, indentLevel + 1)
		}
	}
}

