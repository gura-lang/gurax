TestCase('re.Match() / re.Pattern#Match() / String#Match()') {
	m = re.Match('', '')
	Println(m)
	m = re.Match('abc', 'abcdefg')
	Println(m)
	m = re.Pattern('abc').Match('abcdefg')
	Println(m)
	m = 'abcdefg'.Match('abc')
	Println(m)
}

