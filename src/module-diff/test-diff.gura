#!/usr/bin/env gurax
//==============================================================================
// test-diff.gura
//==============================================================================
import(util.tester) {*}
import(diff)

str1 = R'''
first
second
third
'''

str2 = R'''
first
second
xxxxx
xxxxx
third
'''

TestCase('Basic test') {
	d = diff.DiffLine(str1, str2)
	d.EachHunk {|hunk|
		hunk.Print()
	}
}
