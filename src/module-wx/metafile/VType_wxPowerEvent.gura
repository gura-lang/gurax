PowerEvent = Class(Event) {
	@include['wx/power.h']
	//__init__(evtType as EventType)
	Veto() as void
	IsVetoed() as bool
}

@event {
	EVT_POWER_SUSPENDING		>> PowerEvent
	EVT_POWER_SUSPENDED			>> PowerEvent
	EVT_POWER_SUSPEND_CANCEL	>> PowerEvent
	EVT_POWER_RESUME			>> PowerEvent
}
