#!./gurax
try {
	Error.Raise(Error.SyntaxError, 'message')
	Println('*skipped*')
} catch {|e|
	Println(e)
}
try {
	try {
		Error.Raise(Error.SyntaxError, 'message')
	} catch(Error.ValueError) {|e|
		Println(e)
	}
	Println('*skipped*')
} catch {|e|
	Println(e)
}
try {
	try {
		try {
			Error.Raise(Error.SyntaxError, 'message')
		} catch(Error.ValueError) {|e|
			Println(e)
		}
		Println('*skipped*')
	} catch(Error.TypeError) {|e|
		Println(e)
	}
	Println('*skipped*')
} catch {|e|
	Println(e)
}


try {
	scope {
		Error.Raise(Error.SyntaxError, 'message')
	}
} catch {|e|
	Println(e)
}
try {
	scope {
		scope {
			scope {
				Error.Raise(Error.SyntaxError, 'message')
			}
		}
	}
} catch {|e|
	Println(e)
}
f1() = Error.Raise(Error.SyntaxError, 'message')
f2() = f1()
f3() = f2()
f4() = f3()
try {
	f1()
} catch {|e|
	Println(e)
}
try {
	f2()
} catch {|e|
	Println(e)
}
try {
	f3()
} catch {|e|
	Println(e)
}
try {
	f4()
} catch {|e|
	Println(e)
}
