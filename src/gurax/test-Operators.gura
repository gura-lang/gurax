#!./gurax
import(util)

exprs = [
	`{`a}
	`{`a; `b; `c}
	`{a && b}
	`{a && b && c}
	`{a || b}
	`{a || b || c}
	`(3 + 5)
	`(false && false)
]
for (expr in exprs) {
	Println('--------')
	Printf('%s\n', expr)
	util.PrintPUnit(expr)
}

util.TestCase('Operator ..') {
	util.Tester(`(1..10))
	util.Tester(`(1..1))
	util.Tester(`(1..0))
	util.Tester(`(1..-10))
}

util.TestCase('Operators && and ||') {
	util.Tester(`(true && true))
	util.Tester(`(true && false))
	util.Tester(`(false && true))
	util.Tester(`(false && false))
	util.Tester(`(true || true))
	util.Tester(`(true || false))
	util.Tester(`(false || true))
	util.Tester(`(false || false))
	util.Tester(`('hello' && 'world'))
	util.Tester(`('hello' && false))
	util.Tester(`(false && 'hello'))
	util.Tester(`('hello' && true))
	util.Tester(`(true && 'hello'))
	util.Tester(`('hello' || 'world'))
	util.Tester(`('hello' || false))
	util.Tester(`(false || 'hello'))
	util.Tester(`('hello' || true))
	util.Tester(`(true || 'hello'))
}

