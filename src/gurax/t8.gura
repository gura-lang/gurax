PrintPUnit() {`expr} = {
	seqIdOffset = 0
	punits = expr.EachPUnit()
	punit = punits.NextValue()
	seqIdOffset = punit.seqId
	while (punit) {
		Printf('  ##%d %s\n', punit.seqId - seqIdOffset, punit.ToString(seqIdOffset))
		punit = punits.NextValue()
	}
}
PrintPUnit {
	Println(block)
}
PrintPUnit {
	repeat {}
}
PrintPUnit {
	repeat {|a|}
}
