#!./gurax
PrintPUnit(expr:Expr) = {
	seqIdOffset = 0
	punits = expr.EachPUnit()
	punit = punits.NextValue()
	seqIdOffset = punit.seqId
	while (punit) {
		Printf('#%d %s\n', punit.seqId - seqIdOffset, punit.ToString(seqIdOffset))
		punit = punits.NextValue()
	}
}
x = `{
	while (a < 3) {}
}
PrintPUnit(x)
Println('--------')
x = `(a)
Printf('`(%s)\n', x)
PrintPUnit(x)
Println('--------')
f(`x) = {
	Printf('f(`(%s))\n', x)
	PrintPUnit(x)
}
f(a = 3)
Println('--------')
x = `{
	while (a < 3) {}
}
PrintPUnit(x)
