#!/usr/bin/env gurax

ComposePropertyHelp(vtype as VType) = {
	str = ''
	str += '# Property\n'
	str
}

ComposeConstructorHelp(vtype as VType) = {
	str = ''
	str += '# Constructor\n'
	str
}

ComposeMethodHelp(vtype as VType) = {
	str = ''
	str += '# Method\n'
	str += '\n'
	vtype.__methods__ {|method|
		str += '`' + method.__str__():unbracket + '`\n'
		str += method.__help__().doc
		str += '\n\n'
	}
	str
}
