#!/usr/bin/env gurax
import(util.tester) {*}

TestCase('Property') {
	Tester(`Binary.__prop__(`bytes))
	Tester(`Binary.__prop__(`p))
	Tester(`Binary.__prop__(`reader))
	Tester(`Binary.__prop__(`writable))
	Tester(`Binary.__prop__(`writer))
}

TestCase('Constructor') {
