#!/usr/bin/env gurax
import(util.tester) {*}

TestCase('Map to List') {
	a = ['abcd', 'efg', 'hijklm']
	Tester(`a)
	Tester(`a::len)
	Tester(`a::Upper())
	Tester(`a::Left(2))
	a = []
	Tester(`a)
	Tester(`a::len)
	Tester(`a::Upper())
	Tester(`a::Left(2))
}

TestCase('Map to Iterator') {
	a = ['abcd', 'efg', 'hijklm']
	Tester(`a)
	Tester(`a:*len)
	Tester(`[a:*len*])
	//Tester(`a::Upper())
	//Tester(`a::Left(2))
	a = []
	Tester(`a)
	Tester(`a:*len)
	Tester(`[a:*len*])
	//Tester(`a::Upper())
	//Tester(`a::Left(2))
}
