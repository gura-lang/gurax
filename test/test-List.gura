TesterPerm(expr as Expr) = {
	try {
		tbl = expr.Eval()
		Println('----')
		Printf('%s .. %d : %s\n', expr, tbl.len, tbl):noMap
	} catch {|e|
		Printf('%s .. %s\n', expr, e.text)
	}
}

