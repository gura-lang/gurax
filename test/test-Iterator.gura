#!/usr/bin/env gurax
import(util.tester) {*}

TestCase('PUnit') {
	exprs = [
		`()
		`(1,)
		`(1, 2, 3)
	]
	PrintPUnitWithLabel(exprs)
}

TestCase('Iterator#isFinite() and Iterator#IsInfinite()') {
	x = 1..10
	Tester(`x)
	Tester(`x.IsFinite())
	Tester(`x.IsInfinite())
	x = 1..
	Tester(`x)
	Tester(`x.IsFinite())
	Tester(`x.IsInfinite())
}

TestCase('Iterator#NextValue()') {
	x = 1..10
	while (a = x.NextValue()) {
		Println(a)
	}
}
