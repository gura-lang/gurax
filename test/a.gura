#!/usr/bin/env gurax
f(x):map = { x * x }
//f(x) = {}
t = os.Clock {
	u = repeat (100000):iter {|i| f(i)}
	[u]
}
Println(t)

t = os.Clock {
	repeat (100000):list {|i| f(i)}
}
Println(t)

t = os.Clock {
	Range (100000):list {|i| f(i)}
}
Println(t)

x = Range(100000)
t = os.Clock {
	f(x):list
}
Println(t)
