#!/usr/bin/env gurax
import(util.tester) {*}
sys.path = sys.path |+| ['test-import']

TestCase('PUnit') {
	exprs = [
		`import(a)
		`import(a) {*}
		`import(a) {sym1, sym2, sym3, sym4}
	]
	exprs::PrintPUnitWithLabel()
}

TestCase('Basic Test') {
	import(foo)
	Tester(`dir(foo))
	//Println(foo.Func1)
}

TestCase('Dotted Symbol') {
	import(foo.sub1.sub2)
	Tester(`dir(foo))
	Tester(`dir(foo.sub1))
	Tester(`dir(foo.sub1.sub2))
}

TestCase('Error') {
	Tester(`import(bar))
	foo = 3
	Tester(`import(foo.sub1))
}
