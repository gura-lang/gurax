#!/usr/bin/env gurax
import(util.tester) {*}

TestCase('Unary Operator') {
	Tester(`(-[]))
	Tester(`(-[1, 2, 3, 4, 5]))
	Tester(`[-(1, 2, 3, 4, 5)])
}
TestCase('Binary Operator') {
	Tester(`([] + []))
	Tester(`([1, 2, 3, 4, 5] + [3, 4, 5, 6, 7]))
	Tester(`([1, 2, 3, 4, 5] + [3, 4, 5]))
	Tester(`[[1, 2, 3, 4, 5] + (3, 4, 5, 6, 7)])
	Tester(`[(1, 2, 3, 4, 5) + [3, 4, 5, 6, 7]])
	Tester(`[(1, 2, 3, 4, 5) + (3, 4, 5, 6, 7)])
}
